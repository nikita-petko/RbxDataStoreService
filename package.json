{
  "name": "@mfdlabs/rbxdatastoreservice",
  "version": "3.0.0-alpha.13",
  "description": "A Node.js library for interacting with the Roblox DataStores API",
  "main": "dist/index.js",
  "types": "dist/types",
  "scripts": {
    "test": "jest --config jestconfig.json --verbose --runInBand --passWithNoTests --silent=false",
    "test-full": "npm ci && npm update && npm run test",
    "build": "npm run clean && npm run compile",
    "build-full": "npm ci && npm update && npm run build",
    "clean": "tsc -b --clean tsconfig.json",
    "compile": "tsc -b -v --listEmittedFiles tsconfig.json",
    "lint": "tslint -p tsconfig.json -c tslint.json --fix",
    "lint-ci": "tslint -p tsconfig.json -c tslint.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "publish-package": "npm run lint && npm run test && npm run build && npm publish --access public"
  },
  "keywords": [
    "Roblox",
    "Roblox Web API",
    "BEDEV1",
    "BEDEV2",
    "DataStores"
  ],
  "repository": {
    "type": "git",
    "url": "git+github.com/nkpetko/rbxdatastoreservice.git"
  },
  "author": "Nikita Petko <petko@vmminfra.net>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^28.1.2",
    "@types/node": "^17.0.39",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@mfdlabs/net": "^1.0.6",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1"
  },
  "files": [
    "dist/*",
    "!dist/tests.js"
  ]
}
