	"license": "Apache-2.0",
	"devDependencies": {
		"@types/crypto-js": "^4.0.1",
		"@types/node": "^16.7.10",
		"prettier": "^2.2.0",
		"typescript": "^4.4.3"
	},
	"dependencies": {
		"axios": "^0.24.0",
		"crypto-js": "^4.0.0"
	}
}
